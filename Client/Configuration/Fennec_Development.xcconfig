INCLUDE_SETTINGS_BUNDLE = YES
MOZ_TODAY_WIDGET_SEARCH_DISPLAY_NAME = Freespoke - Search (Staging)
//CODE_SIGN_ENTITLEMENTS = Client/Entitlements/FennecApplication.entitlements
CODE_SIGN_ENTITLEMENTS = Client/Entitlements/Fennec_StagingFennec.entitlements
OTHER_SWIFT_FLAGS = $(OTHER_SWIFT_FLAGS_common) -DMOZ_CHANNEL_FENNEC_DEVELOPMENT -DDEVELOPMENT
MOZ_INTERNAL_URL_SCHEME = fennec
OTHER_LDFLAGS = -ObjC -lxml2 -fprofile-instr-generate
