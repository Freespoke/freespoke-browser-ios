#include "Common.xcconfig"

INFOPLIST_FILE = Client/Fennec_Development-Info.plist
MOZ_BUNDLE_DISPLAY_NAME = Freespoke(Development)
MOZ_BUNDLE_ID = com.freespoke.app.staging
ASSETCATALOG_COMPILER_APPICON_NAME = AppIcon_DEVELOPMENT
PRODUCT_NAME = $(TARGET_NAME)
SKIP_INSTALL = YES
//ASSETCATALOG_COMPILER_APPICON_NAME = AppIcon_STAGING
